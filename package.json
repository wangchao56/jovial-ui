{
  "name": "jovial-ui",
  "type": "module",
  "version": "1.1.2",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/jovial-ui.es.js",
      "require": "./dist/jovial-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./es": "./dist-components/index.js",
    "./es/*": "./dist-components/*/index.js"
  },
  "main": "dist/jovial-ui.umd.js",
  "module": "dist/jovial-ui.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "dist-components"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vite build --mode lib",
    "build:components": "vite build --mode components",
    "build:all": "npm run build:lib && npm run build:components",
    "preview": "vite preview",
    "s:dev": "storybook dev -p 6006",
    "s:build": "storybook build",
    "test-storybook": "vitest --project=storybook",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.ts,.vue --fix",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --cache --fix --allow-empty-input",
    "lint:style:fix": "stylelint src/**/*.{css,scss,vue} --cache --fix --allow-empty-input",
    "lint:all": "pnpm lint && pnpm lint:style",
    "lint:all:fix": "pnpm lint:all && pnpm lint:style:fix",
    "gen:component": "node scripts/generate-component.cjs",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run"
  },
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-docs": "^8.6.12",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^13.0.0",
    "date-fns": "^4.1.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@chromatic-com/storybook": "3",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@storybook/addon-essentials": "8.6.11",
    "@storybook/addon-interactions": "^8.6.11",
    "@storybook/addon-onboarding": "8.6.11",
    "@storybook/blocks": "8.6.11",
    "@storybook/experimental-addon-test": "8.6.11",
    "@storybook/test": "8.6.11",
    "@storybook/vue3": "8.6.11",
    "@storybook/vue3-vite": "8.6.11",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.0",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "postcss-preset-env": "^10.1.5",
    "postcss-scss": "^4.0.9",
    "release-it": "^19.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.86.1",
    "storybook": "8.6.11",
    "stylelint": "^16.17.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.1",
    "terser": "^5.29.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-stylelint": "^6.0.0",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.4"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,vue}": [
      "stylelint --fix --allow-empty-input"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
